	</parameterMap>
	

    <insert id="insert" parameterMap="proteinIonParam">
    	INSERT INTO ProteinProphetProteinIon (
    	piProteinID,
    	piIonID,
    	initialProbability,
    	nspAdjProbability,
    	numSiblingPeptides,
    	isContributingEvidence)
    	VALUES (?,?,?,?,?,?)
    	<selectKey resultClass="int">
      		select last_insert_id() as id
    	</selectKey>
    </insert>
    
</sqlMap>