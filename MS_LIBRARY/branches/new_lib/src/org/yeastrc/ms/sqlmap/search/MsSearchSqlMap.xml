<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
      "http://ibatis.apache.org/dtd/sql-map-2.dtd"> 
<sqlMap namespace="MsSearch"> 

    <typeAlias alias="MsSearchDb" type="org.yeastrc.ms.domain.impl.MsSearchDbImpl"/>
	<typeAlias alias="SequestSearchDb" type="org.yeastrc.ms.domain.sqtFile.impl.SQTSearchDbImpl"/>
	<typeAlias alias="SearchType" type="org.yeastrc.ms.domain.SearchFileFormat"/>
	<typeAlias alias="SearchTypeHandler" type="org.yeastrc.ms.dao.ibatis.MsSearchDAOImpl$SearchFileFormatTypeHandler"/>
	<typeAlias alias="MsSearch" type="org.yeastrc.ms.dao.ibatis.MsSearchDAOImpl$MsSearchSqlMapParam" />
	
	
	<resultMap class="MsSearchDb" id="msSearchResult">
		<result property="id" column="id" />
		<result property="experimentId" column="experimentID" nullValue="0"/>
		<result property="runId" column="runID" nullValue="0"/>
		<result property="searchFileFormat" column="originalFileType"
				javaType="SearchType" jdbcType="VARCHAR"
				typeHandler="SearchTypeHandler"/>
		<result property="searchEngineName" column="analysisProgramName" />
		<result property="searchEngineVersion" column="analysisProgramVersion" />
		<result property="searchDate" column="searchDate" />
		<result property="searchDuration" column="searchDuration" nullValue="0"/>
		<result property="precursorMassType" column="precursorMassMethod" />
		<result property="precursorMassTolerance" column="precursorMassTolerance"/>
		<result property="fragmentMassType" column="fragmentMassMethod" />
		<result property="fragmentMassTolerance" column="fragmentMassTolerance"/>
		<result property="enzymeList" 
				column="id" 
				select="MsEnzyme.selectEnzymesForSearch"/>
		<result property="searchDatabases" 
				select="MsDatabase.selectSearchDatabases" 
				column="id"/>
		<result property="staticModifications" 
				select="MsSearchMod.selectStaticModsForSearch" 
				column="id" />
		<result property="dynamicModifications"
				select="MsSearchMod.selectDynaModsForSearch"
				column="id" />
		<discriminator javaType="SearchType" jdbcType="VARCHAR" 
						column="originalFileType" 
						typeHandler="SearchTypeHandler">
			<subMap value="SQT_NSEQ" resultMap="sqtSearchResult" />	
			<subMap value="SQT_SEQ" resultMap="sqtSearchResult" />						
		</discriminator>
	</resultMap>
	
	<resultMap class="SqtSearchDb" id="sqtSearchResult" extends="MsSearch.msSearchResult">
		<result property="headers" 
				select="SqtHeader.selectHeadersForSearch" 
				column="id"/>
	</resultMap> 
	
	<parameterMap class="MsSearch" id="msSearchParam">
		<parameter property="experimentId" jdbcType="INTEGER"/>
		<parameter property="runId" jdbcType="INTEGER" nullValue="0"/>
		<parameter property="searchFileFormat" 
					jdbcType="VARCHAR" 
					javaType="SearchType" 
					typeHandler="SearchTypeHandler"
		/>
		<parameter property="searchEngineName" jdbcType="VARCHAR" />
		<parameter property="searchEngineVersion" jdbcType="VARCHAR"/>
		<parameter property="searchDate" jdbcType="DATE" />
		<parameter property="searchDuration" jdbcType="INTEGER" nullValue="0"/>
		<parameter property="precursorMassType" jdbcType="VARCHAR" />
		<parameter property="precursorMassTolerance" jdbcType="DECIMAL" />
		<parameter property="fragmentMassType" jdbcType="VARCHAR" />
		<parameter property="fragmentMassTolerance" jdbcType="DECIMAL" />
	</parameterMap>
	
    <select id="select" resultMap="msSearchResult" parameterClass="Integer">
    	SELECT * FROM msPeptideSearch WHERE id = #id#
    </select>
    
    <select id="selectSearchesForRun" resultMap="msSearchResult" parameterClass="Integer">
    	SELECT * FROM msPeptideSearch WHERE runID = #id#
    </select>
    
    <select id="selectSearchIdsForRun" resultClass="Integer" parameterClass="Integer">
    	SELECT id FROM msPeptideSearch WHERE runID = #id#
    </select>
    
    <select id="selectSearchIdsForExperiment" resultClass="Integer" parameterClass="Integer">
    	SELECT id FROM msPeptideSearch WHERE experimentID = #id#
    </select>
    
    <select id="selectSearchIdForRunAndExperiment" resultClass="Integer" parameterClass="java.util.Map">
    	SELECT id FROM msPeptideSearch WHERE runID = #runId# AND experimentID=#experimentId#
    </select>
    
    <insert id="insert" parameterMap="msSearchParam">
    	INSERT INTO msPeptideSearch (
    	experimentID,
    	runID,
    	originalFileType,
    	analysisProgramName,
    	analysisProgramVersion,
    	searchDate,
    	searchDuration,
    	precursorMassMethod,
    	precursorMassTolerance,
    	fragmentMassMethod,
    	fragmentMassTolerance) 
    	VALUES (?,?,?,?,?,?,?,?,?,?,?)
    	<selectKey resultClass="int">
      		select last_insert_id() as id
    	</selectKey>
    </insert>
    
    <delete id="delete" parameterClass="Integer">
		DELETE FROM msPeptideSearch WHERE id=#id#		    
    </delete>
</sqlMap>