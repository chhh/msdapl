<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
      "http://ibatis.apache.org/dtd/sql-map-2.dtd"> 
<sqlMap namespace="MS2ChgDAnalysis"> 

	<typeAlias alias="DAnalysis" type="org.yeastrc.ms.dto.ms2File.MS2FileChargeDependentAnalysis"/>
	
	<resultMap class="DAnalysis" id="danalysisResult">
		<result property="scanChargeId" column="scanChargeID" />
		<result property="name" column="header" />
		<result property="value" column="value" />
	</resultMap>
	
	<parameterMap class="DAanalysis" id="danalysisParam">
		<parameter property="scanChargeId" jdbcType="INTEGER"/>
		<parameter property="name" jdbcType="VARCHAR"/>
		<parameter property="value" jdbcType="VARCHAR"/>
	</parameterMap>
	
    <select id="selectAnalysisForCharge" resultMap="danalysisResult" parameterClass="Integer"> 
        SELECT * FROM MS2FileChargeDependentAnalysis where scanChargeID=#id#
    </select> 
    
    <insert id="insert" parameterMap="danalysisParam">
    	INSERT INTO MS2FileChargeDependentAnalysis (scanChargeID, header, value) 
    	VALUES (?,?,?)
    </insert>
    
    <delete id="deleteById" parameterClass="Integer">
		DELETE FROM MS2FileChargeDependentAnalysis WHERE id=#id#		    
    </delete>
    
</sqlMap>