<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
      "http://ibatis.apache.org/dtd/sql-map-2.dtd"> 
<sqlMap namespace="MsResultProtein"> 

	<typeAlias alias="MsResultProteinDbImpl" type="org.yeastrc.ms.domain.search.impl.MsSearchResultProteinDbImpl"/>
	<typeAlias alias="MsResultProteinDb" type="org.yeastrc.ms.domain.search.MsSearchResultProteinDb"/>
	
	<resultMap class="MsResultProteinDbImpl" id="msResultProteinResult">
		<result property="resultId" column="resultID" nullValue="0"/>
		<result property="proteinId" column="proteinID" nullValue="0"/>
	</resultMap>
	
	<parameterMap class="MsResultProteinDb" id="msResultProteinParam">
		<parameter property="resultId" jdbcType="INTEGER" nullValue="0"/>
		<parameter property="proteinId" jdbcType="INTEGER" nullValue="0"/>
	</parameterMap>
	
    <select id="selectResultProteins" resultMap="msResultProteinResult" parameterClass="Integer">
    	SELECT * FROM msProteinMatch WHERE resultID= #id#
    </select>
    
    
    <insert id="insert" parameterMap="msResultProteinParam">
    	INSERT INTO msProteinMatch (
    	resultID,
    	proteinID,)
    	VALUES (?,?)
    </insert>
    
    <insert id="insertAll" parameterClass="String">
    	INSERT INTO msProteinMatch (resultID, proteinID) VALUES $values$
    </insert>
    
    <delete id="deleteForResultId" parameterClass="Integer">
		DELETE FROM msProteinMatch WHERE resultID=#id#
    </delete>
    
</sqlMap>