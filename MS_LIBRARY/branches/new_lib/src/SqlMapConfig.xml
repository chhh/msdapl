<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
	"http://ibatis.apache.org/dtd/sql-map-config-2.dtd" > 

<sqlMapConfig>

	<properties resource="msDataDB.properties" />
	
	<settings 
		useStatementNamespaces="true"
		lazyLoadingEnabled="true"
	/>
	
	<transactionManager type="JDBC">
		<dataSource type="SIMPLE"> 
      		<property name="JDBC.Driver" 
                value="${db.driver}"/> 
      		<property name="JDBC.ConnectionURL" 
                value="${db.url}"/> 
      		<property name="JDBC.Username" 
                value="${db.user}"/> 
      		<property name="JDBC.Password" 
                value="${db.password}"/> 
    	</dataSource>
	</transactionManager>
	
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsExperimentSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsRunSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsDigestionEnzymeSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsScanSqlMap.xml"/>
	
	<!-- Related to MS2 files -->
	<sqlMap resource="org/yeastrc/ms/sqlmap/ms2File/MS2FileHeaderSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/ms2File/MS2FileChargeIndependentAnalysisSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/ms2File/MS2FileScanChargeSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/ms2File/MS2FileChargeDependentAnalysisSqlMap.xml"/>

	<sqlMap resource="org/yeastrc/ms/sqlmap/MsPeptideSearchResultSqlMap.xml" />
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsProteinMatchSqlMap.xml" />
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsPeptideSearchModSqlMap.xml" />
	<sqlMap resource="org/yeastrc/ms/sqlmap/MsSequenceDatabaseSqlMap.xml"/>
	
	<!-- Related to SQT files -->
	<sqlMap resource="org/yeastrc/ms/sqlmap/sqtFile/SQTSearchHeaderSqlMap.xml"/>
	<sqlMap resource="org/yeastrc/ms/sqlmap/sqtFile/SQTSearchResultSqlMap.xml"/>

</sqlMapConfig>	