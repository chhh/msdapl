<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
      "http://ibatis.apache.org/dtd/sql-map-2.dtd"> 
<sqlMap namespace="PercolatorOutput"> 

    <typeAlias alias="PercolatorOutputOut" type="org.yeastrc.ms.domain.postsearch.percolator.impl.PercolatorOutputBean"/>
    <typeAlias alias="PercolatorOutputIn" type="org.yeastrc.ms.dao.postsearch.percolator.ibatis.PercolatorOutputWrap"/>
	
	<typeAlias alias="FileFormatType" type="org.yeastrc.ms.domain.search.SearchFileFormat"/>
	<typeAlias alias="FileFormatTypeHandler" type="org.yeastrc.ms.dao.search.ibatis.MsRunSearchDAOImpl$SearchFileFormatTypeHandler"/>
	
	<resultMap class="PercolatorOutputOut" id="percOutputResult">
		<result property="id" column="id" />
		<result property="percolatorId" column="percID" />
		<result property="runSearchId" column="runSearchID" />
		<result property="analysisFileFormat" column="originalFileType"
				javaType="FileFormatType" jdbcType="VARCHAR"
				typeHandler="FileFormatTypeHandler"/>
	</resultMap>
	
	<parameterMap class="PercolatorOutputIn" id="percOutputParam">
		<parameter property="percolatorId" />
		<parameter property="runSearchId"  />
		<parameter property="analysisFileFormat" 
					jdbcType="VARCHAR" 
					javaType="FileFormatType" 
					typeHandler="FileFormatTypeHandler" />
	</parameterMap>
	
	
    <select id="select" resultMap="percOutputResult" parameterClass="Integer">
    	SELECT * FROM PercolatorOutput WHERE id = #id#
    </select>
    
    <select id="selectOutputIdsForPercolator" resultClass="Integer" parameterClass="Integer">
    	SELECT id FROM PercolatorOutput WHERE percID = #percolatorId#
    </select>
    
    <insert id="insert" parameterMap="percOutputParam">
    	INSERT INTO PercolatorOutput (
    	percID,
    	runSearchID,
    	originalFileType)
    	VALUES (?,?,?)
    	<selectKey resultClass="int">
      		select last_insert_id() as id
    	</selectKey>
    </insert>
    
</sqlMap>